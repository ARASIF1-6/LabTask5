﻿@model List<LabTask5.Models.Product>
@{
    ViewBag.Title = "ViewProduct";
}


<h2>Product Information</h2>
<hr />


<table class="table table-hover">
    <tr>
        <th>Name</th>
        <th>Category</th>
        <th>Price</th>
    </tr>

    @foreach (var items in Model)
    {
        <tr>

            <td>@items.Name</td>
            <td>@items.Category.CategoryType</td>
            <td>@items.price</td>

        </tr>

    }

</table>

